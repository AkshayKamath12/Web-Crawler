	<script type="text/javascript">
		jQuery(document).ready(function($) {
			setTimeout(function() {
				// Flexislider
				$('.flex-control-nav, .flex-direction-nav').attr('aria-hidden', 'true');
				$('<a class="mlslider-screen-reader-text flex-skip-slider" href="#">Skip Passed Slider</a>').insertBefore('.slides');
				$('<div tabindex="-1" class="mlslider-screen-reader-text flex-skip-slider-location">End of slider. Content continues below.</div>').insertAfter('.flex-direction-nav');
				$('.flex-skip-slider-location').each(function(e) {
					$(this).addClass('flex-skip-slider-location-' + e);
				});
				$('.flex-skip-slider').each(function(e) {
					$(this).addClass('flex-skip-slider-' + e);
					$('.flex-skip-slider-' + e).click(function() {
						$('.flex-skip-slider-location-' + e).focus();
						return false;
					});
				});

				// R slide
				$('.rslides_nav, .rslides_tabs').attr('aria-hidden', 'true');
				$('<a class="mlslider-screen-reader-text rslide-skip-slider" href="#">Skip Passed Slider</a>').insertBefore('.rslides');
				$('<div tabindex="-1" class="mlslider-screen-reader-text rslide-skip-slider-location">End of slider. Content continues below.</div>').insertAfter('.rslides_tabs');
				$('.rslide-skip-slider-location').each(function(e) {
					$(this).addClass('rslide-skip-slider-location-' + e);
				});
				$('.rslide-skip-slider').each(function(e) {
					$(this).addClass('rslide-skip-slider-' + e);
					$('.rslide-skip-slider-' + e).click(function() {
						$('.rslide-skip-slider-location-' + e).focus();
						return false;
					});
				});
			}, 500);
		});
	</script>

